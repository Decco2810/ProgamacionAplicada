#define ROJO 15
#define AMARILLO 2
#define VERDE 4
#define ROJO_2 5
#define AMARILLO_2 18
#define VERDE_2 19
#define ROJO_3 21
#define AMARILLO_3 22

void setup() {

  pinMode(ROJO,OUTPUT);
  pinMode(AMARILLO,OUTPUT);
  pinMode(VERDE,OUTPUT);
  pinMode(ROJO_2,OUTPUT);
  pinMode(AMARILLO_2,OUTPUT);
  pinMode(VERDE_2,OUTPUT);
  pinMode(ROJO_3,OUTPUT);
  pinMode(AMARILLO_3,OUTPUT);
}
void loop() {

  digitalWrite(ROJO,HIGH);
  delay(500);
  digitalWrite(ROJO,LOW);
  delay(500);

  digitalWrite(AMARILLO,HIGH); 
  delay(500);
  digitalWrite(AMARILLO,LOW);
  delay(500);

  digitalWrite(VERDE,HIGH);
  delay(500);
  digitalWrite(VERDE,LOW);
  delay(500);

  digitalWrite(ROJO_2,HIGH);
  delay(500);
  digitalWrite(ROJO_2,LOW);
  delay(500);


  digitalWrite(AMARILLO_2,HIGH);
  delay(500);
  digitalWrite(AMARILLO_2,LOW);
  delay(500);

  digitalWrite(VERDE_2,HIGH);
  delay(500);
  digitalWrite(VERDE_2,LOW);
  delay(500);

  digitalWrite(ROJO_3,HIGH);
  delay(500);
  digitalWrite(ROJO_3,LOW);
  delay(500);

  digitalWrite(AMARILLO_3,HIGH);
  delay(500);
  digitalWrite(AMARILLO_3,LOW);
  delay(500);

  digitalWrite(ROJO,HIGH);
  delay(500);

  digitalWrite(AMARILLO,HIGH);
  delay(500);

  digitalWrite(VERDE,HIGH);
  delay(500);

  digitalWrite(ROJO_2,HIGH);
  delay(500);

  digitalWrite(AMARILLO_2,HIGH);
  delay(500);

  digitalWrite(VERDE_2,HIGH);
  delay(500);

  digitalWrite(ROJO_3,HIGH);
  delay(500);

  digitalWrite(AMARILLO_3,HIGH);
  delay(500);

  digitalWrite(ROJO,LOW);
  delay(500);

  digitalWrite(AMARILLO,LOW);
  delay(500);

  digitalWrite(VERDE,LOW);
  delay(500);

  digitalWrite(ROJO_2,LOW);
  delay(500);

  digitalWrite(AMARILLO_2,LOW);
  delay(500);

  digitalWrite(VERDE_2,LOW);
  delay(500);

  digitalWrite(ROJO_3,LOW);
  delay(500);

  digitalWrite(AMARILLO_3,LOW);
  delay(500);

    digitalWrite(ROJO,HIGH);
  delay(500);

  digitalWrite(AMARILLO,HIGH);
  delay(500);

  digitalWrite(VERDE,HIGH);
  delay(500);
  
  digitalWrite(ROJO_2,HIGH);
  delay(500);

  digitalWrite(AMARILLO_2,HIGH);
  delay(500);

  digitalWrite(VERDE_2,HIGH);
  delay(500);

  digitalWrite(ROJO_3,HIGH);
  delay(500);

  digitalWrite(AMARILLO_3,HIGH);
  delay(500);

  digitalWrite(AMARILLO_3,LOW);
  delay(500);

  digitalWrite(ROJO_3,LOW);
  delay(500);

  digitalWrite(VERDE_2,LOW);
  delay(500);

  digitalWrite(AMARILLO_2,LOW);
  delay(500);

  digitalWrite(ROJO_2,LOW);
  delay(500);

  digitalWrite(VERDE,LOW);
  delay(500);

  digitalWrite(AMARILLO,LOW);
  delay(500);
  
  digitalWrite(ROJO,LOW);
  delay(500);
}
